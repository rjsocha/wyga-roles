*filter
:FILTER-INPUT-RULES - [0:0]
:FILTER-OUTPUT-RULES - [0:0]
:FILTER-FORWARD-RULES - [0:0]
:DOCKER-USER-RULES - [0:0]
-F FILTER-INPUT-RULES
-F FILTER-OUTPUT-RULES
-F FILTER-FORWARD-RULES
-F DOCKER-USER-RULES
{% include "firewall/rules-filter" %}
COMMIT
*nat
:NAT-INPUT-RULES ACCEPT [0:0]
:NAT-OUTPUT-RULES ACCEPT [0:0]
:NAT-PREROUTING-RULES ACCEPT [0:0]
:NAT-POSTROUTING-RULES ACCEPT [0:0]
-F NAT-INPUT-RULES
-F NAT-OUTPUT-RULES
-F NAT-PREROUTING-RULES
-F NAT-POSTROUTING-RULES
{% include "firewall/rules-nat" %}
COMMIT
